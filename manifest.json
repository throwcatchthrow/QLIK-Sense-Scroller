{
	"manifest_version":2,
	"name":"QLIK Scroller",
	"version":"0.1",
	"permissions": 
	[
    	"tabs",
    	"storage"
  	],
	"content_scripts": [
		{
			"matches": [
				"*://dev.qlik.borgwarner.net/*",
				"*://qlik.borgwarner.net/*"
			],
			"run_at": "document_end",
			"js": [
				"content.js"
			]
		}
	],
	"icons":{
		"64":"icon.png"
	},
	"browser_action": {
    	"default_icon": "icon.png",
    	"default_popup": "popup.html"
  	}

	
}